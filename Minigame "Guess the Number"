#include <iostream>
#include <time.h> 

using namespace std;


int main()
{
    srand(time(NULL));

    int rand_num = 1 + rand() % 15;
    bool stop = false;
    int user_input;

    do {
        cout << "Enter number: "; 
        cin >> user_input;
        if (user_input != rand_num)
            cout << "Fuck you lether shit, go again ";
        else
            stop = true;
    } while (!stop);

    cout << "Okay, okay, u win... Gz <3"; 

    return 0;
